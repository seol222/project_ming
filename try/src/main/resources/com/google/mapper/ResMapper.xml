<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.google.mapper.ResMapper">
<insert id="addres">
INSERT INTO restable (id,title,start1,end1,allDay,backgroundColor,writer) VALUES(null ,#{title} ,#{start1},#{end1},#{allDay},#{backgroundColor},#{writer})
</insert>

<select id="getdata" resultType="com.google.domain.ResVO">
SELECT * FROM restable
</select>

</mapper>